{
  "dashboard": {
    "title": "AI Governance MVP - Observability",
    "tags": ["governance", "api", "metrics"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Total Requests",
        "targets": [
          {
            "expr": "increase(requests_total[5m])"
          }
        ],
        "type": "stat",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 6,
          "h": 4
        }
      },
      {
        "id": 2,
        "title": "Allowed Decisions",
        "targets": [
          {
            "expr": "increase(governance_allowed_total[5m])"
          }
        ],
        "type": "stat",
        "gridPos": {
          "x": 6,
          "y": 0,
          "w": 6,
          "h": 4
        }
      },
      {
        "id": 3,
        "title": "Blocked Decisions",
        "targets": [
          {
            "expr": "increase(governance_blocked_total[5m])"
          }
        ],
        "type": "stat",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 6,
          "h": 4
        }
      },
      {
        "id": 4,
        "title": "Rate Limit Hits",
        "targets": [
          {
            "expr": "increase(rate_limit_hits_total[5m])"
          }
        ],
        "type": "stat",
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 4
        }
      },
      {
        "id": 5,
        "title": "Request Latency (p95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, increase(request_latency_ms_bucket[5m]))"
          }
        ],
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 4,
          "w": 12,
          "h": 6
        }
      },
      {
        "id": 6,
        "title": "Requests by Endpoint",
        "targets": [
          {
            "expr": "sum(increase(requests_total[5m])) by (endpoint)"
          }
        ],
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 4,
          "w": 12,
          "h": 6
        }
      },
      {
        "id": 7,
        "title": "Async Logger Queue Depth",
        "targets": [
          {
            "expr": "async_logger_queue_size"
          }
        ],
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 10,
          "w": 12,
          "h": 5
        }
      },
      {
        "id": 8,
        "title": "Logs Written per Minute",
        "targets": [
          {
            "expr": "increase(logs_written_total[1m])"
          }
        ],
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 10,
          "w": 12,
          "h": 5
        }
      },
      {
        "id": 9,
        "title": "Governance Check Latency by Stage",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, increase(governance_check_latency_ms_bucket[5m])) by (stage)"
          }
        ],
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 15,
          "w": 24,
          "h": 6
        }
      },
      {
        "id": 10,
        "title": "Blocked Decisions by Reason",
        "targets": [
          {
            "expr": "sum(increase(governance_blocked_total[5m])) by (reason)"
          }
        ],
        "type": "piechart",
        "gridPos": {
          "x": 0,
          "y": 21,
          "w": 12,
          "h": 6
        }
      },
      {
        "id": 11,
        "title": "Error Rate (5xx)",
        "targets": [
          {
            "expr": "increase(requests_total{status=~\"5.*\"}[5m]) / on() group_left increase(requests_total[5m])"
          }
        ],
        "type": "stat",
        "gridPos": {
          "x": 12,
          "y": 21,
          "w": 12,
          "h": 6
        }
      }
    ],
    "refresh": "30s",
    "schemaVersion": 27,
    "style": "dark",
    "templating": {
      "list": []
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}
